  <script>
    
    function trackEvent(eventName, payload) {
        const data = {
            "event_name": eventName,
            "csrfmiddlewaretoken": "bIk6B5gOV3gxCMKbbjD5x1Z5qCZNLqJfM8fD7Ph4gXqPuJuoLOx2ueGmV6pXZSnG",
        };

        if (payload && payload.additional_info) {
          data.additional_info = payload.additional_info;
        }

        if (payload && payload.candidate_id) {
          data.candidate_id = payload.candidate_id;
        }

        if (payload && payload.source) {
          data.source = payload.source;
        }

        $.ajax({
            url: "/api/event/",
            contentType: "application/json",
            dataType: "json",
            data: JSON.stringify(data),
            headers: { "X-CSRFToken": data.csrfmiddlewaretoken },
            type: "POST"
        });
    }


    function callAnalytics(element, eventType) {
      const eventName = $(element).data('analytics');
      const eventCandidateId = $(element).data('candidate-id');
      const eventSource = $(element).data('source-page');
      const eventParam = $(element).data('parameter');
      const href = $(element).attr('href');

      let payload = {};

      if (eventType) { payload.additional_info = { type: eventType } }
      if (eventCandidateId) { payload.candidate_id = eventCandidateId; }
      if (eventSource) { payload.source = eventSource; }
      if (eventParam) { payload.additional_info.parameter = eventParam; }
      if (href) { payload.additional_info.href = href; }

      trackEvent(eventName, payload);
    }

    $('.js-analytics-event').on('click', function() {
      callAnalytics(this, 'click');
    });

    $('.js-analytics-event-focus').on('focus', function() {
      callAnalytics(this, 'focus');
    });

    $('.js-analytics-show').each(function() {
      callAnalytics(this, 'show');
    })
  </script>

  <script>
    
  async function getTestGroup(flag_name) {
    const data = {
      "csrfmiddlewaretoken": 'bIk6B5gOV3gxCMKbbjD5x1Z5qCZNLqJfM8fD7Ph4gXqPuJuoLOx2ueGmV6pXZSnG',
      flag: flag_name
    };

    return $.ajax({
      url: "/ajax/has_flag/",
      data: data,
      type: "POST"
    });
  }

  async function isTestGroup(groupName) {
    return await getTestGroup(groupName) === 'b';
  }


    
  </script>
