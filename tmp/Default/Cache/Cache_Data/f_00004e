<script>
  $(function(){
    var $jobsFilter = $('.jobs-filter');

    if($jobsFilter.length) {
      $('.js-jobs-show-filters').on('click', function(){
        $('.js-jobs-filter-wrapper').slideDown();
        $(this).hide();
        $jobsFilter.addClass('is-visible');
        $('.js-jobs-hide-filters').show();
        $('html, body').animate({ scrollTop: $jobsFilter.offset().top - 60 }, 250);
        return false;
      });

      $('.js-jobs-hide-filters').on('click', function(){
        $('.js-jobs-filter-wrapper').slideUp();
        $(this).hide();
        $jobsFilter.removeClass('is-visible');
        $('.js-jobs-show-filters').show();
        return false;
      });
    }

    $('.js-toggle-filters').on('click', function() {
      if (!$jobsFilter.hasClass('is-visible')) {
        $('.js-jobs-show-filters').trigger('click');
      } else {
        $('.js-jobs-hide-filters').trigger('click');
      }
    });

  });
</script>


<script>
  function initializeAdvancedSearch() {
    const formEl = document.getElementById("searchform");
    const additionalTextEl = document.getElementById("advanced-search-additional-text");
    const checkboxEl = document.getElementById("advanced-search-checkbox");
    const sectionEl = document.getElementById("advanced-search-section");
    const simpleSearchGroupEl = document.getElementById("simple-search-group");

    const allOfKeywordsInputEl = document.getElementById("all-keywords");
    const excludeKeywordsInputEl = document.getElementById("exclude-keywords");
    const anyKeywordsInputEl = document.getElementById("any-of-keywords");
    const fullTextCheckbox = document.getElementById("full-text-checkbox")

    const finalInputEl = document.getElementById("search-keywords");
    // The one to actually be used by backend

    if (!formEl) {
      return;
    }

    function onCheckboxChange(e) {
      sectionEl.classList.toggle("hidden");
      additionalTextEl.classList.toggle("hidden");
      simpleSearchGroupEl.classList.toggle("advanced-opened");

      if (!e.target.checked) {
        excludeKeywordsInputEl.value = "";
        anyKeywordsInputEl.value = "";
        fullTextCheckbox.checked = false;
      }
    }

    function removeMinus(value) {
      return value.trim().replace("-","");
    }

    function spreadInitialValueAmongInputs() {
      if (!finalInputEl) return;

      const value = finalInputEl.value;
      const parenthesesRegExp = /\([^\)]+\)/gi;
      const startingWithMinusRegExp = /(?:^|\s)(-[a-z0-9]\w*)/gi;

      let anyOfValue = value.match(parenthesesRegExp);
      anyOfValue = anyOfValue ? anyOfValue[0].slice(1, anyOfValue[0].length - 1) : "";
      anyOfValue = anyOfValue.split("|").join(" ");

      let excludeValue = value.match(startingWithMinusRegExp);
      excludeValue = excludeValue ? excludeValue.map(removeMinus).join(" ") : "";

      let allOfValue = value
        .replace(parenthesesRegExp, "")
        .replace(startingWithMinusRegExp, "")
        .trim();

      anyKeywordsInputEl.value = anyOfValue;
      excludeKeywordsInputEl.value = excludeValue;
      allOfKeywordsInputEl.value = allOfValue;

      finalInputEl.value = "";

      if (anyOfValue || excludeValue) {
        checkboxEl.checked = true;
      }
    }

    function onSubmit(e) {
      e.preventDefault();

      let finalValue = allOfKeywordsInputEl.value.trim();
      finalValue += excludeKeywordsInputEl.value.trim() ? " -" + excludeKeywordsInputEl.value.trim().replace(/\s/g," -") : "";
      finalValue += anyKeywordsInputEl.value.trim() ? " (" + anyKeywordsInputEl.value.trim().replace(/\s/g,"|") + ")" : "";

      finalInputEl.value = finalValue.trim();

      allOfKeywordsInputEl.value = "";
      excludeKeywordsInputEl.value = "";
      anyKeywordsInputEl.value = "";

      formEl.submit();
    }

    spreadInitialValueAmongInputs();
    if(checkboxEl.checked) {
      sectionEl.classList.remove('hidden');
      additionalTextEl.classList.remove("hidden");
      simpleSearchGroupEl.classList.add("advanced-opened");
    }
    checkboxEl.addEventListener("change", onCheckboxChange);
    formEl.addEventListener("submit", onSubmit);
  }

  $(document).ready(initializeAdvancedSearch);
</script>






    
      
  
